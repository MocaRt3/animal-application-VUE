{"ast":null,"code":"import PetCard from '@/components/ui/PetCard.vue';\nimport Pagination from '@/components/ui/Pagination.vue';\nexport default {\n  components: {\n    PetCard,\n    Pagination\n  },\n  props: {\n    pets: {\n      type: Array,\n      required: true\n    }\n  },\n  data() {\n    return {\n      // pages: 0,\n      per_one_page: 8,\n      active_page: 1\n    };\n  },\n  computed: {\n    getPages() {\n      return Math.ceil(this.pets.length / this.per_one_page);\n    },\n    getFirstItemPage() {\n      return this.per_one_page * (this.active_page - 1);\n    }\n  },\n  watch: {\n    active_page() {\n      this.$nextTick(() => {\n        document.querySelector('#topside').scrollIntoView({\n          behavior: 'smooth'\n        });\n      });\n    }\n  },\n  methods: {\n    changePage(i) {\n      if (i < 0 && this.active_page === 1) {\n        return;\n      }\n      if (i > 0 && this.active_page === this.getPages) {\n        return;\n      }\n      this.active_page += i;\n    },\n    goFirst() {\n      this.active_page = 1;\n    },\n    goLast() {\n      this.active_page = this.getPages;\n    }\n  }\n};","map":{"version":3,"names":["PetCard","Pagination","components","props","pets","type","Array","required","data","per_one_page","active_page","computed","getPages","Math","ceil","length","getFirstItemPage","watch","$nextTick","document","querySelector","scrollIntoView","behavior","methods","changePage","i","goFirst","goLast"],"sources":["C:\\web\\2021\\animal-application-VUE\\shelter-dom-main\\src\\components\\pets\\PetsGrid.vue"],"sourcesContent":["<template>\n    <section class=\"bg-dim_l_gray2 pt-20 pb-[100px]\" id=\"topside\">\n        <div class=\"container px-4 sm:px-[30px] md:px-0 mx-auto text-center\">\n            <h2 class=\"georgia mb-[60px] text-4xl text-dim_dark leading-tight text-center\">\n                Our friends who <br>\n                are looking for a house\n            </h2>\n            <div class=\"grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4 gap-10 mb-[60px] sm:px-16 md:px-0\" v-if=\"pets.length\">\n                <div class=\"col-span-1\" v-for=\"(item, i) in pets.slice(getFirstItemPage, (this.per_one_page * active_page))\" :key=\"i\">\n                    <PetCard :pet=\"item\" />\n                </div>\n            </div>\n            \n            <!-- pagination -->\n\n            <Pagination @changePage=\"changePage\" :pages=\"getPages\" :active_page=\"active_page\" \n            @goFirst=\"goFirst\" @goLast=\"goLast\" />\n\n\n        </div>\n    </section>\n</template>\n\n<script>\nimport PetCard from '@/components/ui/PetCard.vue'\nimport Pagination from '@/components/ui/Pagination.vue'\n\nexport default {\n    \n    components: {\n        PetCard,\n        Pagination\n    },\n    props: {\n        pets: {\n            type: Array,\n            required: true\n        }\n    },\n    data(){\n        return {\n            // pages: 0,\n            per_one_page: 8,\n            active_page: 1\n        }\n    },\n    computed: {\n        getPages(){\n            return Math.ceil(this.pets.length / this.per_one_page)\n        },\n        getFirstItemPage(){\n            return this.per_one_page * (this.active_page - 1)\n        }\n    },\n    watch: {\n        active_page(){\n            this.$nextTick(() => {\n                document.querySelector('#topside').scrollIntoView({\n                    behavior: 'smooth'\n                });\n            })\n        }\n    },\n    methods: {\n        changePage(i){\n            if(i < 0 && this.active_page === 1){\n                return\n            }\n            if(i > 0 && this.active_page === this.getPages){\n                return\n            }\n            this.active_page += i\n        },\n        goFirst(){\n            this.active_page = 1\n        },\n        goLast(){\n            this.active_page = this.getPages\n        }\n    }\n    \n}\n</script>"],"mappings":"AAwBA,OAAOA,OAAM,MAAO,6BAA4B;AAChD,OAAOC,UA